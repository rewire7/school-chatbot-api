{
  "openapi": "3.1.0",
  "info": {
    "title": "학교 리스트 API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://school-chatbot-api.vercel.app"
    }
  ],
  "paths": {
    "/api/getSchoolsFromNeis": {
      "get": {
        "operationId": "getChungnamMiddleSchools",
        "summary": "충남 중학교 목록 가져오기",
        "description": "충청남도에 있는 중학교들의 정보를 표로 반환합니다.",
        "responses": {
          "200": {
            "description": "학교 마크다운 표 응답",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "| 학교명 | 주소 | 전화번호 | ... |"
                }
              }
            }
          }
        }
      }
    }
  }
}
